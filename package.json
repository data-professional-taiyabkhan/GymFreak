{
  "name": "gymfreak-app",
  "version": "1.0.0",
  "description": "A platform-independent gym app for tracking TDEE and meal logging",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "stop": "pkill -f 'node.*index.js' && pkill -f 'react-scripts'",
    "start:prod": "NODE_PATH=./node_modules node server/index.js"
  },
  "keywords": [
    "gym",
    "fitness",
    "nutrition",
    "tdee",
    "meal-logging"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "react-scripts": "5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
